{
  "name": "mental-health",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "mongoose": "^5.5.7",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-input-mask": "^2.0.4",
    "reactstrap": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coloradosunrae/react-health.git"
  },
  "bugs": {
    "url": "https://github.com/coloradosunrae/react-health.git/issues"
  },
  "homepage": "https://github.com/coloradosunrae/react-health.git#readme"
}
